<div class="folder-route">
  <span class="folder-route-text"
    ><a href="/share/<%= locals.shareUrl %>"
      ><%= locals.headFolderName %></a
    ></span
  >
  <% locals.folderRoute.forEach((folder) => { %>
  <span class="folder-route-text"
    >&nbsp;&gt;&nbsp;
    <a href="/share/<%= locals.shareUrl %>?folder=<%= folder.id %>"
      ><%= folder.name %></a
    ></span
  >
  <% }) %>
</div>
